#:kivy 1.9.1

<CameraScreen>:
    BoxLayout:
        orientation: 'vertical'
        TextInput:
            id: input_teste 
            text: "640, 480"
            multiline: False
        Button:
            text: 'Set Camera Resolution'
            on_press: root.set_camera_resolution()
        Button:
            text: 'Back to home'
            on_press: root.manager.current = 'home'